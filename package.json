{
  "name": "file-upload-refreshed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "format": "prettier '**/**/*.{js,ts,jsx,tsx,json}' --write",
    "depcheck": "depcheck --oneline",
    "check": "npm-check -u",
    "generate": "prisma generate",
    "generate:backup": "prisma generate --schema=./db-sync/prisma/schema.prisma",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "db-sync-main-to-backup": "cd db-sync && npm run sync-main-to-backup",
    "db-sync-backup-to-main": "cd db-sync && npm run sync-backup-to-main",
    "db-sync-scheduled": "cd db-sync && npm run sync-scheduled"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.12.1",
    "@prisma/client": "^5.19.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/busboy": "^1.5.4",
    "@types/multer": "^1.4.12",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^10.0.0",
    "busboy": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.5.4",
    "next": "^14.2.9",
    "next-themes": "^0.3.0",
    "npm-check": "^6.0.1",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "react": "^18.3.1",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.5.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.9",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2"
  }
}
